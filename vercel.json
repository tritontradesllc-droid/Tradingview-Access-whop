{
  "builds": [
    {
      "src": "server.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.9"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server.py"
    }
  ],
  "buildCommand": "pip install -r requirements.txt --no-cache-dir",
  "installCommand": "pip install -r requirements.txt --no-cache-dir"
}
